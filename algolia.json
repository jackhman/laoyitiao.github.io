{
  "index_name": "docs_index",
  "start_urls": ["https://laoyitiao.github.io/posts/"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": ".VPSidebarNav .title",
      "global": true,
      "default_value": "专栏"
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "lvl4": ".content h4",
    "lvl5": ".content h5",
    "content": ".content p, .content li",
    "text": ".content [class^=language-]",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true,
      "default_value": "zh-CN"
    }
  },
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  }
}