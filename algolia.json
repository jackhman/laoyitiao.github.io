{
  "index_name": "docs_index",
  "start_urls": ["https://laoyitiao.github.io/posts/"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": ".content .docIndex a",
      "default_value": "专栏",
      "global": true
    },
    "lvl1": {
      "selector": ".content h1",
      "default_value": "文章",
      "global": true
    },
    "lvl2": {
      "selector": ".content h2",
      "default_value": "文章",
      "global": true
    },
    "lvl3": {
      "selector": ".content h3",
      "default_value": "文章",
      "global": true
    },
    "lvl4": {
      "selector": ".content h4",
      "default_value": "文章",
      "global": true
    },
    "lvl5": {
      "selector": ".content h5",
      "default_value": "文章",
      "global": true
    },
    "text": ".content p,.content li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true,
      "default_value": "zh-CN"
    }
  },
  "indexHeadings": true,
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  }
}